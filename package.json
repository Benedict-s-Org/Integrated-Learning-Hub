{
  "name": "paragraph-memorization-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run src/test/integration",
    "db:push": "npx supabase db push --include-all",
    "db:pull": "npx supabase db pull",
    "update-all": "npx supabase db push --include-all && npm run build && npm run deploy",
    "sync": "git pull origin main && npx supabase db push --include-all && npm run deploy",
    "deploy": "git add . && git commit -m \"Auto-deploy: Update online version\" && git push origin main",
    "deploy:full": "git add . && git commit -m \"feat: admin class number updates\" && git push origin main && npx supabase functions deploy auth && npx supabase db push --include-all",
    "process-images": "node scripts/process-uploads.js",
    "process-images:watch": "node scripts/process-uploads.js --watch"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@supabase/supabase-js": "^2.56.0",
    "@types/file-saver": "^2.0.7",
    "@types/qrcode": "^1.5.6",
    "file-saver": "^2.0.5",
    "html5-qrcode": "^2.3.8",
    "jszip": "^3.10.1",
    "lovable-tagger": "^1.1.13",
    "lucide-react": "^0.344.0",
    "mammoth": "^1.11.0",
    "papaparse": "^5.5.3",
    "pdfjs-dist": "^5.4.624",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.13.0",
    "sharp": "^0.33.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/parser": "^7.29.0",
    "@babel/plugin-syntax-jsx": "^7.28.6",
    "@babel/plugin-syntax-typescript": "^7.28.6",
    "@babel/traverse": "^7.29.0",
    "@babel/types": "^7.29.0",
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^24.0.0",
    "knip": "^5.0.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^1.3.1"
  }
